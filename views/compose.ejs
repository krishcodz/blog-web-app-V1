<%- include('partials/header'); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <lable>Title</lable>
        <input type="text" name="publishTitle" class="form-control">
    </div>
    
    <div class="form-group">
        <lable>Post</lable>
        <textarea class="form-control" name="publishContent" rows="5" class="form-control"></textarea>
    </div>
    
    <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer'); -%>